<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="br.com.lellis"
          android:versionCode="3"
          android:versionName="@string/app_version_name">
    <uses-sdk android:minSdkVersion="7" android:targetSdkVersion="10" />
    <application android:label="@string/app_name"
                 android:theme="@style/AppTheme"
                 android:icon="@drawable/icon">
        <activity android:name=".pouparnoposto.activity.MyActivity"
                  android:label="@string/app_name"
                  android:icon="@drawable/icon">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
                android:name=".pouparnoposto.activity.ResultadoDisplay"
                android:label="@string/title_resultado_display_message"
                android:icon="@drawable/icon">
            <meta-data
                    android:name="android.support.PARENT_ACTIVITY"
                    android:value=".activity.MyActivity" />
        </activity>
    </application>
</manifest>
